<vm-install>        
    <entry>
        <key>topologyUri</key>
        <value>urn:ogf:network:aws.amazon.com:aws-cloud</value>
    </entry>
    <entry>
        <key>vmType</key>
        <value>aws</value>
    </entry>
    <entry>
        <key>region</key>
        <value>us-east-1</value>
    </entry>
    <entry>
        <key>ostype</key>
        <value>windows</value>
    </entry>
    <entry>
        <key>vmQuantity</key>
        <value>1</value>
    </entry>
    <entry>
        <key>vpcID</key>
        <value>urn:ogf:network:aws.amazon.com:aws-cloud:vpc-45143020</value>
    </entry>
    <subnets>
        <subnet>
            <uri>urn:ogf:network:aws.amazon.com:aws-cloud:subnet-a8a632f1</uri>
            <address>10.0.1.0</address>
        </subnet>
    </subnets>
    <volumes>
        <volume>
            <path>/dev/xvda</path>
            <snapshot>snapshot</snapshot>
            <size>8</size>
            <type>standard</type>
        </volume>
    </volumes>
</vm-install>